{
    "name": "gintonicweb/users",
    "description": "Users accounts",
    "type": "cakephp-plugin",
    "homepage": "http://gintoniccms.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Philippe Lafrance",
            "homepage": "http://phillafrance.com",
            "role": "Author"
        },
        {
            "name": "GintonicWeb Contributors",
            "homepage": "http://github.com/gintonicweb/users/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/gintonicweb/users/issues",
        "source": "https://github.com/gintonicweb/users"
    },
    "require": {
        "stripe/stripe-php": "3.*",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp-codesniffer": "2.*"
