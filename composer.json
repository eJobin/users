{
    "name": "gintonicweb/users",
    "description": "Users plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "3.1.*@beta",
        "friendsofcake/crud-users": "dev-master",
        "friendsofcake/authenticate": "dev-cake3"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp-codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Users\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Users\\Test\\": "tests"
        }
    }
}
